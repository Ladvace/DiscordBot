    "axios": "^0.21.0",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mongoose": "^5.10.11",
    "nodemon": "^2.0.6",
    "npm-check-updates": "^9.2.3",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "tmi.js": "^1.5.0"
  },
  "devDependencies": {
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "2.1.2"
